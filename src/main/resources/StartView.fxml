        <ImageView layoutX="3.0">
            <image>
                <Image url="@images/StartGame/StartGameV2.jpg"/>
            </image>
        </ImageView>
        <TextField fx:id="teamNameTextField" alignment="CENTER" layoutX="662.0" layoutY="223.0" opacity="0.22"
